{
  "name": "@elizaos/client-twitter",
  "version": "0.1.7-alpha.2",
  "main": "src/index.ts",
  "type": "module",
  "dependencies": {
    "agent-twitter-client": "0.0.17",
    "glob": "11.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "tsup": "8.3.5"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "lint": "eslint --fix  --cache ."
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "pluginType": "elizaos:plugin:1.0.0",
  "pluginParameters": {
      "type": "object",
      "required": ["githubOwner", "githubRepo", "githubBranch", "githubPath", "githubApiToken"],
      "properties": {
        "githubOwner": {
          "type": "string",
          "description": "GitHub owner/organization name"
        },
        "githubRepo": {
          "type": "string",
          "description": "GitHub repository name"
        },
        "githubBranch": {
          "type": "string",
          "description": "GitHub branch name"
        },
        "githubPath": {
          "type": "string",
          "description": "GitHub repository path"
        },
        "githubApiToken": {
          "type": "string",
          "description": "GitHub API token"
        }
      }
    },
    "pluginEnv": {
      "GITHUB_OWNER": "githubOwner",
      "GITHUB_REPO": "githubRepo",
      "GITHUB_BRANCH": "githubBranch",
      "GITHUB_PATH": "githubPath",
      "GITHUB_API_TOKEN": "githubApiToken"
    }
}
