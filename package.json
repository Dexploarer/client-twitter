{
    "name": "@elizaos-plugins/client-twitter",
    "version": "0.25.6-alpha.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "agent-twitter-client": "0.0.18",
        "discord.js": "14.16.3",
        "glob": "11.0.0"
    },
    "devDependencies": {
        "@vitest/coverage-v8": "1.1.3",
        "tsup": "8.3.5",
        "vitest": "1.6.1"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "agentConfig": {
      "pluginType": "elizaos:client:1.0.0",
      "pluginParameters": {
        "ASTERAI_AGENT_ID": {
          "type": "string",
          "minLength": 1,
          "description": "ASTERAI_AGENT_ID is required"
        },
        "ASTERAI_PUBLIC_QUERY_KEY": {
          "type": "string",
          "minLength": 1,
          "description": "ASTERAI_PUBLIC_QUERY_KEY is required"
        },
        "TWITTER_DRY_RUN": {
          "type": "boolean",
          "description": "Enable dry run mode for Twitter operations"
        },
        "TWITTER_USERNAME": {
          "type": "string",
          "minLength": 1,
          "description": "X/Twitter username is required"
        },
        "TWITTER_PASSWORD": {
          "type": "string",
          "minLength": 1,
          "description": "X/Twitter password is required"
        },
        "TWITTER_EMAIL": {
          "type": "string",
          "format": "email",
          "description": "Valid X/Twitter email is required"
        },
        "MAX_TWEET_LENGTH": {
          "type": "integer",
          "default": 280,
          "description": "Maximum length of tweets"
        },
        "TWITTER_SEARCH_ENABLE": {
          "type": "boolean",
          "default": false,
          "description": "Enable Twitter search functionality"
        },
        "TWITTER_2FA_SECRET": {
          "type": "string",
          "description": "Twitter 2FA secret for authentication"
        },
        "TWITTER_RETRY_LIMIT": {
          "type": "integer",
          "description": "Number of retry attempts for Twitter operations"
        },
        "TWITTER_POLL_INTERVAL": {
          "type": "integer",
          "description": "Interval for polling Twitter updates (in milliseconds)"
        },
        "TWITTER_TARGET_USERS": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^[A-Za-z][A-Za-z0-9_]*[A-Za-z0-9]$|^[A-Za-z]$",
            "minLength": 1,
            "maxLength": 15
          },
          "default": [],
          "description": "List of Twitter usernames to target"
        },
        "ENABLE_TWITTER_POST_GENERATION": {
          "type": "boolean",
          "description": "Enable automatic Twitter post generation"
        },
        "POST_INTERVAL_MIN": {
          "type": "integer",
          "description": "Minimum interval between posts (in milliseconds)"
        },
        "POST_INTERVAL_MAX": {
          "type": "integer",
          "description": "Maximum interval between posts (in milliseconds)"
        },
        "ENABLE_ACTION_PROCESSING": {
          "type": "boolean",
          "description": "Enable processing of Twitter actions"
        },
        "ACTION_INTERVAL": {
          "type": "integer",
          "description": "Interval for processing actions (in milliseconds)"
        },
        "POST_IMMEDIATELY": {
          "type": "boolean",
          "description": "Post content immediately without delay"
        },
        "TWITTER_SPACES_ENABLE": {
          "type": "boolean",
          "default": false,
          "description": "Enable Twitter Spaces functionality"
        },
        "MAX_ACTIONS_PROCESSING": {
          "type": "integer",
          "description": "Maximum number of actions to process at once"
        },
        "ACTION_TIMELINE_TYPE": {
          "type": "string",
          "enum": ["ForYou", "Following"],
          "default": "ForYou",
          "description": "Type of timeline to process actions from"
        }
      }
    }
}
